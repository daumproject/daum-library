<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.daum.common.gps.javase</groupId>
    <artifactId>org.daum.common.gps.javase.root</artifactId>
    <packaging>jar</packaging>
    <name>Daum :: Common :: GPS :: JavaSE </name>
    <version>1.0</version>

    <parent>
        <artifactId>org.daum.common.gps</artifactId>
        <groupId>org.daum.common.gps</groupId>
        <version>1.0</version>
        <relativePath>../pom.xml</relativePath>
    </parent>

    <build>
        <sourceDirectory>../org.daum.common.gps.root/src/main/</sourceDirectory>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>

        </plugins>
    </build>

</project>
