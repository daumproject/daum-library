<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.daum.common.message.javase</groupId>
    <artifactId>org.daum.common.message.javase.root</artifactId>
    <packaging>jar</packaging>
    <name>Daum :: Common :: JavaSE :: Message </name>
    <version>1.6</version>

    <parent>
        <artifactId>org.daum.common.message</artifactId>
        <groupId>org.daum.common.message</groupId>
        <version>1.6</version>
        <relativePath>../pom.xml</relativePath>
    </parent>

    <build>
        <sourceDirectory>../org.daum.common.message.root/src/main/</sourceDirectory>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>

        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>org.daum.library.javase</groupId>
            <artifactId>org.daum.library.javase.ormH</artifactId>
            <version>2.3</version>
        </dependency>
    </dependencies>

</project>
