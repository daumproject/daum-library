<project>

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.kevoree.nativeN</groupId>
    <artifactId>org.kevoree.nativeN.testcomponent_bridge</artifactId>
    <name>Kevoree :: Native :: Bridge Java</name>
    <packaging>jar</packaging>


    <parent>
        <groupId>org.kevoree.nativeN</groupId>
        <artifactId>org.kevoree.nativeN.testcomponent</artifactId>
        <version>2.1-SNAPSHOT</version>
    </parent>


    <properties>
        <kevoree.version>1.8.8-SNAPSHOT</kevoree.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <scala.version>2.9.2</scala.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.kevoree.nativeN</groupId>
            <artifactId>org.kevoree.nativeN.framework</artifactId>
            <version>1.2-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.kevoree.tools</groupId>
            <artifactId>org.kevoree.tools.javase.framework</artifactId>
            <version>${kevoree.version}</version>
        </dependency>
        <dependency>
            <groupId>org.kevoree.tools</groupId>
            <artifactId>org.kevoree.tools.annotation.api</artifactId>
            <version>${kevoree.version}</version>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.kevoree.tools</groupId>
                <artifactId>org.kevoree.tools.annotation.mavenplugin</artifactId>
                <version>${kevoree.version}</version>
                <extensions>true</extensions>
                <configuration>
                    <nodeTypeNames>JavaSENode</nodeTypeNames>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>generate</goal>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.kevoree.platform</groupId>
                <artifactId>org.kevoree.platform.mavenrunner</artifactId>
                <version>${kevoree.version}</version>
            </plugin>
        </plugins>
    </build>

</project>
